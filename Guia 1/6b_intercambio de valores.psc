Algoritmo intercambiodevalores
	Definir num1, num2 como entero;
	Escribir " Ingresar valor correspondientes al num1 y num2 ";
	Leer num1;
	Leer num2;
	num1 = num1 + num2;
	num2 = num1 - num2;
	num1 = num1 - num2;
	Escribir " Los numeros intercambiados son num1 ", num1, " Y num2 ", num2;
	
FinAlgoritmo
